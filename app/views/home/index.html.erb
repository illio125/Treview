
<div class="container home">
  <div class="row text-center">
    <div class="col-md-12">
      <h1>Video for Traveller</h1>
    </div>
  </div>

  <div class="row travel_search" data-travels="<%= @travels.to_json %>">
    <div class='col-xs-10 col-xs-offset-1 col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3'>
      <div class="input-group">
        <input type="text" class="form-control typeahead" placeholder="Midnight in Paris" >
        <span class="input-group-btn">
          <button class="btn btn-danger search" type="button">Go</button>
        </span>
      </div>
    </div>
  </div>
</div>

<div id="background"></div>

