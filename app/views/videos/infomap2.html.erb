<script type="text/javascript">
$(document).ready(function(){
    /* Get iframe src attribute value i.e. YouTube video url
    and store it in a variable */
    var url = $("#modalVideo").attr('src');
    var url2 = $("#modalVideo2").attr('src');
    var url3 = $("#modalVideo3").attr('src');
    var url4 = $("#modalVideo4").attr('src');
    var url5 = $("#modalVideo5").attr('src');

    /* Remove iframe src attribute on page load to
    prevent autoplay in background */
    $("#modalVideo").attr('src', '');
    $("#modalVideo2").attr('src', '');
    $("#modalVideo3").attr('src', '');
    $("#modalVideo4").attr('src', '');
    $("#modalVideo5").attr('src', '');

  /* Assign the initially stored url back to the iframe src
    attribute when modal is displayed */
    $("#myModal").on('shown.bs.modal', function(){
        $("#modalVideo").attr('src', url);
    });
    $("#myModal2").on('shown.bs.modal', function(){
        $("#modalVideo2").attr('src', url2);
    });
    $("#myModal3").on('shown.bs.modal', function(){
        $("#modalVideo3").attr('src', url3);
    });
    $("#myModal4").on('shown.bs.modal', function(){
        $("#modalVideo4").attr('src', url4);
    });
    $("#myModal5").on('shown.bs.modal', function(){
        $("#modalVideo5").attr('src', url5);
    });

    /* Assign empty url value to the iframe src attribute when
    modal hide, which stop the video playing */
    $("#myModal").on('hide.bs.modal', function(){
        $("#modalVideo").attr('src', '');
    });
    $("#myModal2").on('hide.bs.modal', function(){
        $("#modalVideo2").attr('src', '');
    });
    $("#myModal3").on('hide.bs.modal', function(){
        $("#modalVideo3").attr('src', '');
    });
    $("#myModal4").on('hide.bs.modal', function(){
        $("#modalVideo4").attr('src', '');
    });
    $("#myModal5").on('hide.bs.modal', function(){
        $("#modalVideo5").attr('src', '');
    });
});
</script>



